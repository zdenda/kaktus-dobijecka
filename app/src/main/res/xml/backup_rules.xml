<?xml version="1.0" encoding="utf-8"?>
<!-- Control backup on Android 12 or higher -->
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="backup_preferences.xml" />
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="backup_preferences.xml" />
    </device-transfer>
</data-extraction-rules>
